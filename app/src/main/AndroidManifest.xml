<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appzheimer.uem.appzheimer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnfermedadActivity"
            android:label="@string/title_activity_enfermedad"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DietaActivity"
            android:label="@string/title_activity_dieta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EjercicioActivity"
            android:label="@string/title_activity_ejercicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PrevencionActivity"
            android:label="@string/title_activity_prevencion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AyudaActivity"
            android:label="@string/title_activity_ayuda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactoActivity"
            android:label="@string/title_activity_contacto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QuienesSomosActivity"
            android:label="@string/title_activity_quienes_somos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EvaluacionActivity"
            android:label="@string/title_activity_evaluacion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InicioActivity"
            android:label="@string/title_activity_enfermedad"
            android:theme="@style/AppTheme.NoActionBar" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".SplashNotificaciones"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"></activity>
        <activity
            android:name=".ContactoEnviarMail"
            android:label="@string/title_activity_contacto_enviar_mail"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>